{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "include": ["docs/dist"],
    // "include": ["components"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown", 
    // "node_modules/better-docs/typescript",
    "node_modules/better-docs/component"
  ],
  "templates": {
    "better-docs": {
      "name": "Vault Maker Components"
    }
  },
  "opts": {
    "destination": "docs/out",
    "templates": "node_modules/better-docs",
    "recurse": true,
    "readme": "README.md"
  }
}
