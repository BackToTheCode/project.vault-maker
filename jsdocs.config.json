{
  "plugins": [],
  "recurseDepth": 10,
  "opts": {
    "destination": "./docs/out/",
    "recurse": true
  },
  "source": {
    "include": "docs/dist",
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
